<manifest package="com.renj.classification"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".app.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".view.activity.ClassificationActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.activity.ClassificationListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.CollectionListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.SeeListActivity"
            android:screenOrientation="portrait" />

    </application>

</manifest>
